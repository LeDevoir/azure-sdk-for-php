{
    "name": "philippe-bourdeau/azure-message-queue",
    "description": "Forked from Azure/azure-sdk-for-php",
    "keywords": [ "php", "azure", "sdk", "mq"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philippe Bourdeau",
            "email": "bourdeau.phil@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "pear/net_url2": "^2.2",
        "pear/mail_mime": "^1.10",
        "firebase/php-jwt": "^4.0 || ^5.0",
        "microsoft/azure-storage": "^0.19.1",
        "guzzlehttp/guzzle": "^6.2",
        "zendframework/zend-mime": "^2.6",
        "zendframework/zend-mail": "^2.7"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^4.8.35",
        "phpdocumentor/phpdocumentor": "^2.8",
        "sebastian/phpcpd": "^2.0",
        "phploc/phploc": "^2.1",
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "squizlabs/php_codesniffer": "^2.6",
        "mayflower/php-codebrowser": "^1.1"
    },
    "autoload": {
        "psr-4": { "WindowsAzure\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
