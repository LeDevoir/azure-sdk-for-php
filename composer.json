{
    "name": "ledevoir/azure-message-queue",
    "description": "Forked from Azure/azure-sdk-for-php - credit to Microsoft for the code base",
    "keywords": [ "php", "azure", "sdk", "mq"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Philippe Bourdeau",
            "email": "pbourdeau@ledevoir.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "pear/net_url2": "^2.2",
        "pear/mail_mime": "^1.10",
        "firebase/php-jwt": "^4.0 || ^5.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6.9",
        "phpunit/phpunit": "^4.8.35",
        "phpdocumentor/phpdocumentor": "^2.8",
        "sebastian/phpcpd": "^2.0",
        "phploc/phploc": "^2.1",
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "squizlabs/php_codesniffer": "^2.6",
        "mayflower/php-codebrowser": "^1.1"
    },
    "autoload": {
        "psr-4": { "WindowsAzure\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    }
}
